check process resque_web_<%= @app_name %>
  with pidfile /var/run/engineyard/resque/<%= @app_name %>/resque_web.pid
  start program = "/engineyard/bin/resque-web <%= @app_name %> start" with timeout 90 seconds
  stop program =  "/engineyard/bin/resque-web <%= @app_name %> stop"
  group <%= @app_name %>_resque_web

